{
  "name": "@react-web-kit/ui",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/index.css",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build:ts": "tsc --build",
    "build:css": "postcss src/index.css -o dist/styles.css",
    "build": "pnpm build:ts && pnpm build:css",
    "prepublishOnly": "pnpm run build"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.1",
    "postcss-cli": "^11.0.0",
    "postcss": "^8.4.38",
    "typescript": "^5.2.2"
  }
}
