  "files": ["dist"],
  "scripts": {
    "build": "vite build",
    "build:turbo": "turbo run build --filter=@react-web-kit/theme-provider"
  },
  "peerDependencies": {
    "@types/react": "^19.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/styled-components": "^5.1.34",
    "styled-components": "^6.1.15",
    "vite-plugin-dts": "^4.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
