{
  "name": "@react-web-kit/theme-provider",
  "version": "1.0.0",
  "description": "Paquete para estilos globales",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
