{
  "name": "@react-web-kit/theme-provider",
  "version": "1.0.5",
  "description": "Package for global styles",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "homepage": "https://github.com/Jhonatan815/react-ds-kit-ui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jhonatan815/react-ds-kit-ui.git",
    "directory": "packages/theme-provider"
  },
  "sideEffects": false,
  "files": ["dist"],
  "keywords": [
    "react",
    "theme",
    "provider",
    "styled-components",
    "design-system"
  ],
  "license": "MIT",
  "author": "Jhonatan Benavides",
  "scripts": {
    "build": "vite build",
    "build:turbo": "turbo run build --filter=@react-web-kit/theme-provider"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "styled-components": "^6.1.15"
  },
  "devDependencies": {
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/styled-components": "^5.1.34",
    "vite-plugin-dts": "^4.5.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
